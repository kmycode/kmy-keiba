<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <ControlTemplate x:Key="MarkStar" TargetType="ContentControl">
    <Polygon Stroke="{StaticResource HorseMarkForeground}" StrokeThickness="3" Width="32" Height="32"
              Points="16 0, 20 12, 32 12, 24 20, 28 32, 16 24, 6 32, 10 20, 0 12, 14 12, 16 0"/>
  </ControlTemplate>
  <ControlTemplate x:Key="MarkCircle" TargetType="ContentControl">
    <Ellipse Width="32" Height="32" StrokeThickness="3" Stroke="{StaticResource HorseMarkForeground}"/>
  </ControlTemplate>
  <ControlTemplate x:Key="MarkDoubleCircle" TargetType="ContentControl">
    <Grid Width="32" Height="32">
      <Ellipse Width="32" Height="32" StrokeThickness="3" Stroke="{StaticResource HorseMarkForeground}"/>
      <Ellipse Width="20" Height="20" StrokeThickness="3" Stroke="{StaticResource HorseMarkForeground}"
                HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="MarkTriangle" TargetType="ContentControl">
    <Polygon Stroke="{StaticResource HorseMarkForeground}" StrokeThickness="3"
              Points="16 4, 32 28, 0 28, 16 4" Height="32"/>
  </ControlTemplate>
  <ControlTemplate x:Key="MarkFilledTriangle" TargetType="ContentControl">
    <Polygon Stroke="{StaticResource HorseMarkForeground}" StrokeThickness="3"
              Points="16 4, 32 28, 0 28, 16 4" Fill="{StaticResource HorseMarkForeground}" Height="32"/>
  </ControlTemplate>
  <ControlTemplate x:Key="MarkCheck" TargetType="ContentControl">
    <Polyline Stroke="{StaticResource HorseMarkForeground}" StrokeThickness="6"
              Points="4 16, 16 28, 28 4" Height="32"/>
  </ControlTemplate>
  <ControlTemplate x:Key="MarkNote" TargetType="ContentControl">
    <TextBlock Foreground="{StaticResource HorseMarkForeground}" Height="32"
               FontSize="28" VerticalAlignment="Center" HorizontalAlignment="Center" Text="注"
               FontFamily="MS Gothic" FontWeight="DemiBold">
      <TextBlock.RenderTransform>
        <TranslateTransform Y="4"/>
      </TextBlock.RenderTransform>
    </TextBlock>
  </ControlTemplate>
  <ControlTemplate x:Key="MarkDeleted" TargetType="ContentControl">
    <Grid Width="32" Height="32" Margin="-2,0,0,0">
      <TextBlock Foreground="{StaticResource HorseMarkDeletedForeground}"
               FontSize="36" VerticalAlignment="Center" HorizontalAlignment="Center" Text="消"
               FontWeight="Bold">
        <TextBlock.RenderTransform>
          <TranslateTransform Y="-2"/>
        </TextBlock.RenderTransform>
      </TextBlock>
    </Grid>
  </ControlTemplate>
  <ControlTemplate x:Key="MarkDefault" TargetType="ContentControl">
    <Line StrokeDashArray="2 4" Stroke="{StaticResource HorseMarkDeletedForeground}" StrokeThickness="3"
          Width="32" Height="32" X1="0" X2="32" Y1="16" Y2="16"/>
  </ControlTemplate>

