<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:u="clr-namespace:KmyKeiba.Common">
  <u:RHColor x:Key="HorseNumberPlateBackground" Red="148" Green="148" Blue="66"/>
  <u:RHColor x:Key="HorseNumberPlateBorder" Red="240" Green="240" Blue="240"/>
  <u:RHColor x:Key="HorseNumberPlateItemBackground" Red="200" Green="200" Blue="200"/>
  <u:RHColor x:Key="HorseNumberPlateItemBorder" Red="0" Green="0" Blue="0"/>
  <u:RHColor x:Key="HorseNumberPlateItemForeground" Red="0" Green="0" Blue="0"/>

  <Style TargetType="StackPanel">
    <Setter Property="Background" Value="#030303"/>
  </Style>
  <Style TargetType="TextBlock">
    <Setter Property="Foreground" Value="#f0f0f0"/>
  </Style>
</ResourceDictionary>